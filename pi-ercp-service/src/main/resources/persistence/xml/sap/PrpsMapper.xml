  <select id="queryPrpsList" resultMap="BaseResultMap">
    select * from prps left join proj on prps.psphi=proj.pspnr
    <where>
      <if test="psphi != null and psphi != ''">
        proj.pspid = #{psphi}
      </if>
      <if test="pspnr != null and pspnr != ''">
        and prps.pspnr = #{pspnr}
      </if>
      <if test="posid != null and posid != ''">
        and prps.posid like concat('%', #{posid}, '%')
      </if>
      <if test="post1 != null and post1 != ''">
        and prps.post1 like concat('%', #{post1}, '%')
      </if>
    </where>
    order by prps.pspnr
