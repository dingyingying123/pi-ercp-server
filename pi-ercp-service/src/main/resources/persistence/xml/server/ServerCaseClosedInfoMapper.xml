  <select id="queryCaseClosedList" resultMap="BaseResultMapVO">
    select * from server_case_closed_info i left join pi_children_base_info b on i.child_id =b.child_id join server_child_status_info s on s.sta_id=i.sta_id
    <where>
      <if test="childName != null and childName != '' ">
        (i.child_name like concat('%', #{childName}, '%') or b.child_id_no like concat('%', #{childName}, '%'))
      </if>
      <if test="childId != null and childId != '' ">
        and i.child_id = #{childId}
      </if>
      <if test="area != null and area != '' ">
        and b.area like concat('%', #{area}, '%')
      </if>
      <if test="caseClosedStatus != null and caseClosedStatus != ''">
        and s.case_closed_status = #{caseClosedStatus}
      </if>
    </where>
    order by i.close_date desc
  </select>
